<view>
    <!-- list start -->
    <view class="group-lists-box">
 
        <view class="group-lists-content">
            <view wx:for="{{peoples}}" class="group-item-box" wx:key="item.peopleId">
                <image class="group-user-img" src="{{item.basic.avatar}}" ></image>
                
                <view class="group-item-content">
                    <text class="group-item-name">{{item.basic.name}}</text>
                    <text class="group-item-title">加入时间：{{item.created}}</text>
                </view>

            </view>
        </view>
    </view>
    <!-- /list  end -->
</view>
